{
    "name": "ewereka/laravel-extra-validation",
    "description": "A set of extended validation rules for use with Laravel.",
    "keywords": ["laravel", "validation", "rules", "ewereka"],
    "type": "library",
    "license": "MIT",
	"homepage": "https://github.com/ewereka/laravel-extra-validation",
    "authors": [
        {
            "name": "Sean Metzgar",
            "email": "sean.metzgar@gmail.com",
            "role": "Developer"
        }
    ],
	"autoload": {
		"psr-4": {
			"Ewereka\\Validation\\Rules\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ewereka\\Validation\\Rules\\Tests\\": "tests"
		}
	},
    "require": {
		"php": "^7.2"
	},
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0"
    },
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
    "minimum-stability": "stable"
}
